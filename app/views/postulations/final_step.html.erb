<div class='container'>
	<div class='jumbotron'>

		<h2>Datos Personales</h2>
		<label>Nombre </label>
		<%= current_applicant.name %>
		<label>E-mail </label>
		<%= current_applicant.email %>
		<hr>
		<h2>Datos Postulacion</h2>
		<label>Motivacion </label>
		<%= @ultima_postulacion_usuario.postulation_detail.motivation %>
		</br>
		<%= link_to "Edit", edit_postulation_path(@ultima_postulacion_usuario)%>
		<hr>

		<h2>Lenguajes</h2>
		<% @ultima_postulacion_usuario.postulation_detail.languages.each do |l| %>
			<label>Nombre Idioma</label>
			<%= l.name %>
			<label>Escrito</label>
			<%= l.written_level %>
			<label>Hablado</label>
			<%= l.spoken_level %>
		<% end %>
		</br>
		<%= link_to "Edit", edit_postulation_detail_path(@ultima_postulacion_usuario.postulation_detail)%>
	</div>
</div>